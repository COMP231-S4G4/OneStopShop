        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BlogTitle)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BlogCreatedDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BlogCreatedDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BlogModifiedDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BlogModifiedDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BlogDescription)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BlogDescription)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.BlogId">Edit</a>&nbsp;
    <a class="btn btn-primary" asp-action="Index" asp-route-StoreId="@Model.StoreId">Back to List</a>
