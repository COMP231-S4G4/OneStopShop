}

<link rel="stylesheet" type="text/css" href="~/css/BlogDetails.css" />

<h1>Details</h1>

<div>
    <h4>Blogs</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StoreId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StoreId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BlogImage)
        </dt>
